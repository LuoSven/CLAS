@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_LayoutCommon.cshtml";
}
@using CLAS.Model.Entities;
@model List<CL_User> 
<div class="SearchForm">
    <form id="SearchForm" action="@Url.Action("Index","SystemUser")">
        <input type="text" name="UserName" value="" placeholder="用户名">
        <button class="searchButton" type="submit">搜索</button>
        <button data-commond="updatesystemprograms" class="searchButton" id="UpdatePrograms" type="button">刷新系统作业</button>
    </form>

</div>

<div id="List">
    @Html.Partial("_List", Model)
</div>

@section Js{
    <script>
        $("#UpdatePrograms").click(function () {
            $.ajax({
                url: "@Url.Action("UpdateSystemPrograms")",
                success: function(a) {
                    alert(a.message);
                }
            });
        })
    </script>
}
